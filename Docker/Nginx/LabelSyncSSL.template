server {
        listen 443 ssl http2;
        server_name "${NGINX_HOST}";

        ssl_certificate /etc/letsencrypt/live/labelsync/fullchain.pem;
        ssl_certificate_key /etc/letsencrypt/live/labelsync/privkey.pem;

        location / {
                proxy_pass http://0.0.0.0:8080;
        }

}
